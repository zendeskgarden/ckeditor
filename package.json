{
  "name": "@zendeskgarden/ckeditor",
  "description": "Garden editor implementation for CKEditor5",
  "license": "Apache-2.0",
  "author": "Zendesk Garden <garden@zendesk.com>",
  "homepage": "https://garden.zendesk.com/",
  "repository": "https://github.com/zendeskgarden/ckeditor",
  "bugs": {
    "url": "https://github.com/zendeskgarden/ckeditor/issues"
  },
  "version": "0.1.0",
  "main": "dist/theme/theme.css",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "postcss src/theme --dir dist/theme",
    "build:storybook": "build-storybook",
    "format": "prettier-package-json --write && yarn format:all --write",
    "format:all": "prettier --loglevel warn '**/*.{js,ts,json,md}' '.storybook/**/*.{js,ts,json,md}' '!CHANGELOG.md' '!dist/**'",
    "lint": "yarn lint:js && yarn lint:css && yarn lint:md",
    "lint:css": "stylelint 'src/**/*.css'",
    "lint:js": "eslint src/ .storybook/ --ext js,ts --max-warnings 0",
    "lint:md": "markdownlint README.md src/**/*.md",
    "prepare": "yarn build",
    "start": "start-storybook -p 6006",
    "tag": "[ `git rev-parse --abbrev-ref HEAD` = 'main' ] && standard-version --no-verify",
    "test": "prettier-package-json --list-different && yarn format:all --check && yarn lint"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@ckeditor/ckeditor5-basic-styles": "24.0.0",
    "@ckeditor/ckeditor5-block-quote": "24.0.0",
    "@ckeditor/ckeditor5-code-block": "24.0.0",
    "@ckeditor/ckeditor5-dev-utils": "23.6.1",
    "@ckeditor/ckeditor5-dev-webpack-plugin": "23.6.1",
    "@ckeditor/ckeditor5-editor-decoupled": "24.0.0",
    "@ckeditor/ckeditor5-essentials": "24.0.0",
    "@ckeditor/ckeditor5-heading": "24.0.0",
    "@ckeditor/ckeditor5-horizontal-line": "24.0.0",
    "@ckeditor/ckeditor5-indent": "24.0.0",
    "@ckeditor/ckeditor5-link": "24.0.0",
    "@ckeditor/ckeditor5-list": "24.0.0",
    "@ckeditor/ckeditor5-paragraph": "24.0.0",
    "@ckeditor/ckeditor5-react": "3.0.0",
    "@ckeditor/ckeditor5-theme-lark": "24.0.0",
    "@ckeditor/ckeditor5-widget": "24.0.0",
    "@storybook/addon-a11y": "6.1.15",
    "@storybook/addon-essentials": "6.1.10",
    "@storybook/react": "6.1.10",
    "@zendeskgarden/eslint-config": "17.1.0",
    "@zendeskgarden/react-theming": "8.31.0",
    "@zendeskgarden/scripts": "0.1.11",
    "@zendeskgarden/stylelint-config": "15.0.0",
    "@zendeskgarden/svg-icons": "6.29.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.2",
    "envalid": "6.0.2",
    "eslint": "7.16.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-notice": "0.9.10",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "4.3.0",
    "markdownlint-cli": "0.25.0",
    "postcss": "8.2.4",
    "postcss-cli": "8.3.1",
    "postcss-inline-svg": "4.1.0",
    "postcss-simple-vars": "5.0.2",
    "prettier": "2.1.2",
    "prettier-package-json": "2.1.3",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-is": "17.0.1",
    "standard-version": "9.0.0",
    "styled-components": "5.2.1",
    "stylelint": "13.8.0",
    "stylelint-order": "4.1.0"
  },
  "keywords": [
    "ckeditor",
    "ckeditor5",
    "garden",
    "rte",
    "zendesk"
  ],
  "publishConfig": {
    "access": "public"
  }
}
