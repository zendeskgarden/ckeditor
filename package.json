{
  "name": "@zendeskgarden/ckeditor",
  "version": "0.1.0",
  "description": "Garden editor implementation for CKEditor5",
  "license": "Apache-2.0",
  "author": "Zendesk Garden <garden@zendesk.com>",
  "homepage": "https://garden.zendesk.com/",
  "repository": "https://github.com/zendeskgarden/ckeditor",
  "bugs": {
    "url": "https://github.com/zendeskgarden/ckeditor/issues"
  },
  "main": "dist/theme/theme.css",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "postcss src/theme --dir dist/theme",
    "build:storybook": "build-storybook",
    "format": "prettier-package-json --write && yarn format:all --write",
    "format:all": "prettier --loglevel warn '**/*.{js,ts,json,md}' '.storybook/**/*.{js,ts,json,md}' '!CHANGELOG.md' '!dist/**'",
    "postinstall": "husky install",
    "lint": "yarn lint:js && yarn lint:css && yarn lint:md",
    "lint:css": "stylelint 'src/**/*.css'",
    "lint:js": "eslint src/ .storybook/ --ext js,ts --max-warnings 0",
    "lint:md": "markdownlint README.md src/**/*.md",
    "prepare": "yarn build",
    "start": "start-storybook -p 6006",
    "tag": "[ `git rev-parse --abbrev-ref HEAD` = 'main' ] && standard-version --no-verify",
    "test": "prettier-package-json --list-different && yarn format:all --check && yarn lint"
  },
  "devDependencies": {
    "@babel/core": "7.17.10",
    "@ckeditor/ckeditor5-basic-styles": "31.1.0",
    "@ckeditor/ckeditor5-block-quote": "31.1.0",
    "@ckeditor/ckeditor5-code-block": "31.1.0",
    "@ckeditor/ckeditor5-dev-utils": "25.4.5",
    "@ckeditor/ckeditor5-dev-webpack-plugin": "25.4.5",
    "@ckeditor/ckeditor5-editor-decoupled": "31.1.0",
    "@ckeditor/ckeditor5-essentials": "31.1.0",
    "@ckeditor/ckeditor5-heading": "31.1.0",
    "@ckeditor/ckeditor5-horizontal-line": "31.1.0",
    "@ckeditor/ckeditor5-indent": "31.1.0",
    "@ckeditor/ckeditor5-link": "31.1.0",
    "@ckeditor/ckeditor5-list": "31.1.0",
    "@ckeditor/ckeditor5-paragraph": "31.1.0",
    "@ckeditor/ckeditor5-react": "3.0.3",
    "@ckeditor/ckeditor5-theme-lark": "31.1.0",
    "@ckeditor/ckeditor5-widget": "31.1.0",
    "@storybook/addon-a11y": "6.4.22",
    "@storybook/addon-essentials": "6.4.22",
    "@storybook/react": "6.4.22",
    "@zendeskgarden/eslint-config": "17.1.0",
    "@zendeskgarden/react-theming": "8.49.4",
    "@zendeskgarden/scripts": "1.4.0",
    "@zendeskgarden/stylelint-config": "16.0.1",
    "@zendeskgarden/svg-icons": "6.32.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.5",
    "envalid": "7.3.1",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-notice": "0.9.10",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.5.0",
    "husky": "7.0.4",
    "markdownlint-cli": "0.31.1",
    "postcss": "8.4.13",
    "postcss-cli": "9.1.0",
    "postcss-inline-svg": "4.1.0",
    "postcss-simple-vars": "5.0.2",
    "prettier": "2.6.2",
    "prettier-package-json": "2.6.3",
    "prop-types": "15.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-is": "17.0.2",
    "standard-version": "9.3.2",
    "styled-components": "5.3.5",
    "stylelint": "13.13.1",
    "stylelint-order": "5.0.0"
  },
  "keywords": [
    "ckeditor",
    "ckeditor5",
    "garden",
    "rte",
    "zendesk"
  ],
  "publishConfig": {
    "access": "public"
  }
}
