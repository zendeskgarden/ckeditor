{
  "name": "@zendeskgarden/ckeditor",
  "version": "0.2.1",
  "description": "Garden editor implementation for CKEditor5",
  "license": "Apache-2.0",
  "author": "Zendesk Garden <garden@zendesk.com>",
  "homepage": "https://garden.zendesk.com/",
  "repository": "https://github.com/zendeskgarden/ckeditor",
  "bugs": {
    "url": "https://github.com/zendeskgarden/ckeditor/issues"
  },
  "main": "dist/theme/theme.css",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "postcss src/theme --dir dist/theme",
    "build:storybook": "storybook build",
    "format": "prettier-package-json --write && yarn format:all --write",
    "format:all": "prettier --log-level warn '**/*.{js,mjs,ts,json,md}' '.storybook/**/*.{js,ts,json,md}' '!CHANGELOG.md' '!dist/**'",
    "lint": "yarn lint:js && yarn lint:css && yarn lint:md",
    "lint:css": "stylelint 'src/**/*.css'",
    "lint:js": "eslint src/ .storybook/ --ext js,ts --max-warnings 0",
    "lint:md": "markdownlint README.md src/**/*.md",
    "prepare": "yarn build && husky install",
    "start": "storybook dev -p 6006",
    "tag": "[ `git rev-parse --abbrev-ref HEAD` = 'main' ] && standard-version --no-verify",
    "test": "prettier-package-json --list-different && yarn format:all --check && yarn lint"
  },
  "resolutions": {
    "netlify-cli": "9.16.7"
  },
  "devDependencies": {
    "@babel/core": "7.22.15",
    "@babel/eslint-parser": "7.22.15",
    "@babel/preset-react": "7.22.15",
    "@ckeditor/ckeditor5-basic-styles": "40.2.0",
    "@ckeditor/ckeditor5-block-quote": "40.2.0",
    "@ckeditor/ckeditor5-code-block": "40.2.0",
    "@ckeditor/ckeditor5-dev-utils": "39.5.0",
    "@ckeditor/ckeditor5-dev-webpack-plugin": "31.1.13",
    "@ckeditor/ckeditor5-editor-decoupled": "40.2.0",
    "@ckeditor/ckeditor5-essentials": "40.2.0",
    "@ckeditor/ckeditor5-heading": "40.2.0",
    "@ckeditor/ckeditor5-horizontal-line": "40.2.0",
    "@ckeditor/ckeditor5-indent": "40.2.0",
    "@ckeditor/ckeditor5-link": "40.2.0",
    "@ckeditor/ckeditor5-list": "40.2.0",
    "@ckeditor/ckeditor5-paragraph": "40.2.0",
    "@ckeditor/ckeditor5-react": "6.1.0",
    "@ckeditor/ckeditor5-theme-lark": "40.2.0",
    "@ckeditor/ckeditor5-widget": "40.2.0",
    "@storybook/addon-a11y": "7.4.0",
    "@storybook/addon-essentials": "7.4.0",
    "@storybook/react": "7.4.0",
    "@storybook/react-webpack5": "7.4.0",
    "@zendeskgarden/eslint-config": "34.0.0",
    "@zendeskgarden/react-theming": "8.69.10",
    "@zendeskgarden/scripts": "2.0.1",
    "@zendeskgarden/stylelint-config": "20.0.0",
    "@zendeskgarden/svg-icons": "7.0.0",
    "babel-loader": "9.1.3",
    "css-loader": "6.8.1",
    "envalid": "7.3.1",
    "eslint": "8.48.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-notice": "0.9.10",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "markdownlint-cli": "0.36.0",
    "postcss": "8.4.31",
    "postcss-cli": "10.1.0",
    "postcss-inline-svg": "6.0.0",
    "postcss-loader": "7.3.3",
    "postcss-simple-vars": "7.0.1",
    "prettier": "3.0.3",
    "prettier-package-json": "2.8.0",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-is": "18.2.0",
    "standard-version": "9.5.0",
    "storybook": "7.4.0",
    "styled-components": "6.0.7",
    "stylelint": "15.10.3",
    "stylelint-order": "6.0.3",
    "webpack": "5.88.2"
  },
  "keywords": [
    "ckeditor",
    "ckeditor5",
    "garden",
    "rte",
    "zendesk"
  ],
  "publishConfig": {
    "access": "public"
  }
}
