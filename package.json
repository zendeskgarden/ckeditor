{
  "name": "@zendeskgarden/ckeditor",
  "version": "0.2.1",
  "description": "Garden editor implementation for CKEditor5",
  "license": "Apache-2.0",
  "author": "Zendesk Garden <garden@zendesk.com>",
  "homepage": "https://garden.zendesk.com/",
  "repository": "https://github.com/zendeskgarden/ckeditor",
  "bugs": {
    "url": "https://github.com/zendeskgarden/ckeditor/issues"
  },
  "main": "dist/theme/theme.css",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "postcss src/theme --dir dist/theme",
    "build:storybook": "storybook build",
    "format": "prettier-package-json --write && npm run -- format:all --write",
    "format:all": "prettier --log-level warn '**/*.{js,mjs,ts,json,md}' '.storybook/**/*.{js,ts,json,md}' '!CHANGELOG.md' '!dist/**'",
    "postinstall": "husky",
    "lint": "npm run lint:js && npm run lint:css && npm run lint:md",
    "lint:css": "stylelint 'src/**/*.css'",
    "lint:js": "eslint --max-warnings 0",
    "lint:md": "markdownlint README.md src/**/*.md",
    "prepare": "npm run build",
    "start": "storybook dev --no-version-updates -p 6006",
    "tag": "[ `git rev-parse --abbrev-ref HEAD` = 'main' ] && standard-version --no-verify",
    "test": "prettier-package-json --list-different && npm run -- format:all --check && npm run lint"
  },
  "devDependencies": {
    "@babel/core": "7.24.7",
    "@babel/preset-react": "7.24.7",
    "@ckeditor/ckeditor5-dev-translations": "40.4.0",
    "@ckeditor/ckeditor5-dev-utils": "40.4.0",
    "@ckeditor/ckeditor5-react": "8.0.0",
    "@ckeditor/ckeditor5-utils": "42.0.1",
    "@storybook/addon-a11y": "8.1.11",
    "@storybook/addon-essentials": "8.1.11",
    "@storybook/addon-webpack5-compiler-babel": "3.0.3",
    "@storybook/manager-api": "8.1.11",
    "@storybook/react": "8.1.11",
    "@storybook/react-webpack5": "8.1.11",
    "@zendeskgarden/eslint-config": "42.0.0",
    "@zendeskgarden/react-theming": "8.76.4",
    "@zendeskgarden/scripts": "2.4.0",
    "@zendeskgarden/stylelint-config": "21.0.0",
    "@zendeskgarden/svg-icons": "7.2.0",
    "babel-loader": "9.1.3",
    "ckeditor5": "42.0.1",
    "css-loader": "7.1.2",
    "dotenv": "16.4.5",
    "envalid": "8.0.0",
    "eslint": "9.5.0",
    "eslint-config-prettier": "9.1.0",
    "husky": "9.0.11",
    "markdownlint-cli": "0.41.0",
    "postcss": "8.4.39",
    "postcss-cli": "11.0.0",
    "postcss-inline-svg": "6.0.0",
    "postcss-loader": "8.1.1",
    "postcss-simple-vars": "7.0.1",
    "prettier": "3.3.2",
    "prettier-package-json": "2.8.0",
    "prop-types": "15.8.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-is": "18.3.1",
    "standard-version": "9.5.0",
    "storybook": "8.1.11",
    "styled-components": "6.1.11",
    "stylelint": "16.6.1",
    "stylelint-order": "6.0.4",
    "webpack": "5.92.1"
  },
  "keywords": [
    "ckeditor",
    "ckeditor5",
    "garden",
    "rte",
    "zendesk"
  ],
  "publishConfig": {
    "access": "public"
  }
}
